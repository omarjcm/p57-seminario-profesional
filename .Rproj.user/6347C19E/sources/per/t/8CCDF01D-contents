require(RPostgreSQL)
require(tidyverse)

coneccion <- dbConnect(
  dbDriver("PostgreSQL"), 
  user="postgres", 
  password="inexcelsisdeo", 
  dbname="see_peliculas", 
  host="localhost"
)

sql <- '
  SELECT * FROM actores;
'

rs <- dbSendQuery(coneccion, sql)
df <- fetch(rs, n=-1)
df <- as_tibble(df)

dbDisconnect( coneccion )

names( df )
print( df  )